scalar Long

type Query {
    getAuthors: [Author]
    findBooksByAuthorId(authorId: Long): [Book]
    findAuthorById(authorId: Long): Author
}

type Author {
    id: Long
    firstName: String
    lastName: String
    books: [Book]
}

type Book {
    isdn: String
    title: String
    authors: [Author]
}